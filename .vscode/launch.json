    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "serve"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Inspect",
            "request": "launch",
            "runtimeArgs": [
                "--inspect",
                "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
                "serve"
            ],
            "runtimeExecutable": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}